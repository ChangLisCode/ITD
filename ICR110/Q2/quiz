define int n
define the binary number groupï¼Œgroup = [x_1, X_2, ... X_n]

for input_decimal  < 0
     position_n in group X_n = input_decimal % 2,
     input_decimal = input_decimal / 2 
     
transfer group to output_binary
print output_binary

converting binary to decimal

Define int n = input binary number
confirm the number amount of x
Define database = []
decimal_number = 0
pick up each number of n, and fill in the database == [n(y-1),n(y-2),...,n(1),n(0)] // (it is the postion number)
y = 0
for (i=0, i<x, i++)
     pick up the data from database and calculate, decimal_number += n(y)*2^i
     y++
print decimal_number





int i = 1
int n = 2
for (i = 1, i < 101, i++)
     if i == 1
          exit
     else if i == 2
          print i
     else if     
          for (n = 2, n < i, n++)
               let i divided by n
               while the remainder == 0 
                    print i
     else
          exit
     
